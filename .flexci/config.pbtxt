configs {
  key: "pytorch-pfn-extras.prep-linux"
  value {
    requirement {
      cpu: 8
      memory: 32
      disk: 20
    }
    time_limit: {
      seconds: 3600
    }
    command: "bash .flexci/linux/script.sh prep"
  }
}

configs {
  key: "pytorch-pfn-extras.torch17-linux"
  value {
    requirement {
      cpu: 4
      gpu: 2
      memory: 16
      disk: 10
    }
    time_limit: {
      seconds: 1200
    }
    command: "bash .flexci/linux/script.sh torch17"
  }
}

configs {
